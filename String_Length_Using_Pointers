#include<stdio.h>
int strlength(char*string)
{
    char *ptr=string;
    while(*ptr !='\0')
    {
        ptr++;
        
    }
    return ptr-string;
}
void main()
{
    char string[10];
    printf("Enter the string\n");
    scanf("%s", string);
    printf("Length of the string =%d",strlength(string));
}